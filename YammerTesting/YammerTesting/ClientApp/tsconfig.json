{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "lib": [
      "es2015",
      "dom"
    ],
    //"noImplicitAny": false,
    //"typeRoots": [
    //  "./types/allTypes.d.ts",
    //  "node_modules/@types/"
    //],
    "skipLibCheck": false
  },
  "include": [
    "src"
  ]
  //"exclude": [
  //  "types"
  //]
}
